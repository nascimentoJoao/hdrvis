<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="hdrvis" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Linux">
				<Option output="./hdrvis" prefix_auto="1" extension_auto="1" />
				<Option object_output="./" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option parameters="table.hdr" />
				<Compiler>
					<Add option="-O2" />
					<Add directory="include" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="GL" />
					<Add library="GLU" />
					<Add library="glut" />
					<Add directory="lib" />
				</Linker>
			</Target>
			<Target title="Windows">
				<Option output="./hdrvis" prefix_auto="1" extension_auto="1" />
				<Option object_output="./" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option parameters="table.hdr" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-DFREEGLUT_STATIC" />
					<Add directory="include" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="libFreeGLUT.a" />
					<Add library="glu32" />
					<Add library="opengl32" />
					<Add library="winmm" />
					<Add library="gdi32" />
					<Add directory="lib" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-O3" />
			<Add option="-Wall" />
			<Add option="-g" />
			<Add option="-fexceptions" />
			<Add option="-std=c99" />
		</Compiler>
		<Unit filename="main.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="opengl.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="opengl.h" />
		<Unit filename="rgbe.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="rgbe.h" />
		<Extensions>
			<envvars />
			<code_completion />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
